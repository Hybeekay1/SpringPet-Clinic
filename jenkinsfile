@Library('my-shared-library') _
pipeline {
   agent any
   stages {
    stage('Git Checkout'){
      steps {
         gitCheckout(
            branch: "main",
            url: "https://github.com/Hybeekay1/SpringPet-Clinic.git"
         )
            }
        }
      stage('Unit Test Maven'){
      steps {
         mvnTest()
            }
        }
    }
}
